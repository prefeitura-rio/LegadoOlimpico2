{
  "compilerOptions": {
    "baseUrl": "aztec-gods",
    "paths": {
      "$actions/*": ["src/actions/*"],
      "$components/*": ["src/components/*"],
      "$data/*": ["src/data/*"],
      "$domain/*": ["src/domain/*"],
      "$stores/*": ["src/stores/*"],
      "$styles/*": ["src/styles/*"],
      "$svg/*": ["src/svg/*"],
      "$utils/*": ["src/utils/*"]
    }
  },
  "include": ["src/**/*.js", "src/**/*.svelte"],
  "exclude": ["tasks/*.js"]
}
